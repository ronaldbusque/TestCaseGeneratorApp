OPENAI_API_KEY=your_openai_api_key
# Optional: override the default OpenAI model used in the UI
OPENAI_MODEL=gpt-4.1-mini

# Optional: enable Google Gemini
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-1.5-pro-latest

# Optional: enable OpenRouter (or any OpenAI-compatible endpoint)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=openrouter/auto
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_HTTP_REFERER=https://qualityforge.ai
OPENROUTER_HTTP_TITLE=QualityForge AI Tools

# Optional: route all models through the Vercel AI Gateway
# AI_GATEWAY_URL=https://gateway.ai.vercel.com/api/v1
# AI_GATEWAY_API_KEY=your_ai_gateway_key

# Access tokens for middleware authentication (client-provided header values)
ACCESS_TOKENS='{"replace_with_your_token_1":"user_or_team_identifier_1", "your_token_2":"identifier_2"}'
